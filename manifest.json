{
    "name": "politic-names",
    "description": "Gives accurate names to politicians",
    "version": "0.0.1",
    "browser_action": {
        "default_title": "Choose names to replace",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*", "file:///*/*"],
        "js": ["content.js"],
        "run_at": "document_end"
    }],
    "manifest_version": 2
}