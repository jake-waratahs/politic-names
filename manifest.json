{
    "name": "spade",
    "description": "Gives accurate names to politicians",
    "version": "0.0.1",
    "browser_action": {
        "default_title": "Choose names to replace",
        "default_popup": "popup.html",
        "default_icon": "spade.png"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["content.js"],
        "run_at": "document_end"
    }],
    "permissions": ["storage"],
    "icons": {
        "128": "spade128.png"
    },
    "manifest_version": 2
}